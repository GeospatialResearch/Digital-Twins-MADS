{

    "image": {

        "tag": "2.21.2",

        "repository": "docker.osgeo.org/geoserver"

    },

    "ingress": {

        "enabled": true

    },

    "container": {

        "port": 8088

    },

    "resources": {

        "limits": {

            "cpu": "1",

            "memory": "2Gi"

        },

        "requests": {

            "cpu": "0.25",

            "memory": "0.8Mi"

        }

    },

    "autoscaling": {

        "minReplicas": 1

    },

    "env": [

        {

            "name": "SKIP_DEMO_DATA",

            "value": "true"

        },

        {

            "name": "CORS_ENABLED",

            "value": "true"

        }

    ],

    "openExternal": false

}